
def gv

pipeline {
    agent any
    tools {
  maven 'maven3'
    }
    stages {
        stage('init') {
            steps{
                script {
                    gv = load "script.groovy"
                }
            }

        }
        stage('build') {
            steps{
                script {
                    gv.build()
                }
            }

        }
    }
}
